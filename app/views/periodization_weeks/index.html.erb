<p id="notice"><%= notice %></p>

<h1>Periodization Weeks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Type</th>
      <th>Goal ctl bike</th>
      <th>Goal ctl run</th>
      <th>Goal ctl swim</th>
      <th>Cycle position</th>
      <th>Periodization cycle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @periodization_weeks.each do |periodization_week| %>
      <tr>
        <td><%= periodization_week.title %></td>
        <td><%= periodization_week.type %></td>
        <td><%= periodization_week.goal_ctl_bike %></td>
        <td><%= periodization_week.goal_ctl_run %></td>
        <td><%= periodization_week.goal_ctl_swim %></td>
        <td><%= periodization_week.cycle_position %></td>
        <td><%= periodization_week.periodization_cycle %></td>
        <td><%= link_to 'Show', periodization_week %></td>
        <td><%= link_to 'Edit', edit_periodization_week_path(periodization_week) %></td>
        <td><%= link_to 'Destroy', periodization_week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Periodization Week', new_periodization_week_path %>
