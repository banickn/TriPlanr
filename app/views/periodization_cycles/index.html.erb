<p id="notice"><%= notice %></p>

<h1>Periodization Cycles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Type</th>
      <th>Goal ctl bike</th>
      <th>Goal ctl run</th>
      <th>Goal ctl swim</th>
      <th>Atp position</th>
      <th>Atp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @periodization_cycles.each do |periodization_cycle| %>
      <tr>
        <td><%= periodization_cycle.title %></td>
        <td><%= periodization_cycle.type %></td>
        <td><%= periodization_cycle.goal_ctl_bike %></td>
        <td><%= periodization_cycle.goal_ctl_run %></td>
        <td><%= periodization_cycle.goal_ctl_swim %></td>
        <td><%= periodization_cycle.atp_position %></td>
        <td><%= periodization_cycle.atp %></td>
        <td><%= link_to 'Show', periodization_cycle %></td>
        <td><%= link_to 'Edit', edit_periodization_cycle_path(periodization_cycle) %></td>
        <td><%= link_to 'Destroy', periodization_cycle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Periodization Cycle', new_periodization_cycle_path %>
