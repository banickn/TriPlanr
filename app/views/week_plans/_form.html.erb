<%= simple_form_for(@week_plan) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :macro_period %>
    <%= f.input :meso_period %>
    <%= f.input :load_time %>
    <%= f.input :load_atl %>
    <%= f.input :load_ctl %>
    <%= f.input :camp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
